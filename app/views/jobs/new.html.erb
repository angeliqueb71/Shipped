<h2>New Job</h2>

  <%= form_for @job do |f| %>


  <div class="field">
    <%= f.label :Ship_Name %><br />
    <%= f.text_field :title, autofocus: true %> <br />
    <%= f.label :Description %><br />
    <%= f.text_area :body %> <br />
    <%= f.label :Origin %><br />
    <%= f.text_field :location %><br />
    <%= f.label :Destination %><br />
    <%= f.text_field :location %><br />
  </div>

  <div class="field">
    <%= f.label :Container_Amount %><br />
    <%= f.text_field :container, autofocus: true %>
  </div>


  <div class="field">
    <%= f.label :Cost %><br />
    <%= f.text_field :cost, autofocus: true %>
  </div>

  <div class="actions">
    <%= f.submit "Submit" %>
  </div>
 <% end %>

 <%= link_to 'Back', root_path %>
