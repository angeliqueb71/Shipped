<h2>New Ship</h2>

  <%= form_for @ship do |f| %>


  <div class="field">
    <%= f.label :Ship %><br />
    <%= f.text_field :name, autofocus: true %> <br />
  </div>

  <div class="field">
    <%= f.label :Container_Amount %><br />
    <%= f.text_field :container, autofocus: true %>
  </div>


  <div class="field">
    <%= f.label :Location %><br />
    <%= f.text_field :location, autofocus: true %>
  </div>

  <div class="actions">
    <%= f.submit "Submit" %>
  </div>
 <% end %>

 <%= link_to 'Back', root_path %>
 <%= link_to 'Profile', root_path %>
